#include<cstring>
#include <iostream>
using namespace std;
void removeConsecutiveDuplicates(char *input) {
    int s=0;
    if(input[s]=='\0'){
        return ;
    }
    removeConsecutiveDuplicates(input+1);
    if(input[s]==input[s+1]){
        for(int i=s; i<strlen(input);i++){
            input[i]=input[i+1];
        }
    }
    


}



int main() {
    char s[100000];
    cin >> s;
    removeConsecutiveDuplicates(s);
    cout << s << endl;
}
